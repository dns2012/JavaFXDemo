<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Button fx:id="btnClickMe" layoutX="123.0" layoutY="171.0" mnemonicParsing="false" onAction="#generateRandom" text="SUBMIT" />
      <Label fx:id="labelRandom" alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="90.0" prefWidth="286.0" text="RANDOM">
         <font>
            <Font name="Apple Braille" size="48.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="14.0" layoutY="114.0" onAction="#comboChanged" prefHeight="27.0" prefWidth="286.0" promptText="Select Fruit" />
      <ListView fx:id="listView" layoutX="14.0" layoutY="216.0" prefHeight="211.0" prefWidth="471.0" />
      <Button fx:id="btnSelectedItem" layoutX="14.0" layoutY="441.0" mnemonicParsing="false" onAction="#selectedItem" prefHeight="44.0" prefWidth="471.0" text="GET SELECTED ITEM" />
   </children>
</AnchorPane>
